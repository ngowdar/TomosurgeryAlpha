<Window x:Class="TomosurgeryAlpha.RestrictSliceWeight"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RestrictSliceWeight" Height="170" Width="248">
    <Grid>
        <TextBox x:Name="Slicenum_box" HorizontalAlignment="Left" Height="23" Margin="37,36,0,0" TextWrapping="Wrap" Text="Slicenumber?" VerticalAlignment="Top" Width="71" Foreground="#FFA8A4A4"/>
        <TextBox x:Name="Sliceweight_box" HorizontalAlignment="Left" Height="23" Margin="125,36,0,0" TextWrapping="Wrap" Text="0.8" VerticalAlignment="Top" Width="71"/>
        <Button x:Name="Reweightslices_btn" Content="Set and Re-optimize" HorizontalAlignment="Left" Margin="37,83,0,0" VerticalAlignment="Top" Width="172" Height="29" Click="Reweightslices_btn_Click"/>

    </Grid>
</Window>
